{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
    ],
    "globals": {
      "HTMLRewriter": false,
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-restricted-syntax": 0,
      "class-methods-use-this": 0
    }
}
